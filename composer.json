{
    "name": "john-peterson-g17/oauth-token-management",
    "description": "A framework agnostic abstraction for managing tokens/grants in the OAuth 2.0 flow as described in RFC 6749. This package would be a module inside of your Authorization Server used to manage tokens",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JohnPetersonG17\\OAuthTokenManagement\\": "src/",
            "JohnPetersonG17\\OAuthTokenManagement\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "John Peterson",
            "email": "peterson.glock17@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "larapack/dd": "^1.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require": {
        "php": ">=8.2",
        "firebase/php-jwt": "^6.10",
        "ramsey/uuid": "^4.7",
        "predis/predis": "^2.2"
    }
}
